<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.neteye.health.dao.UsersDao">

	<select id="get" resultType="com.neteye.health.domain.UsersDO">
		select `id`,`username`,`password`,`gender`,`marry`,`birthday`,`register_time`,`last_login_time`,`last_login_ip`,`type`,`company_id`,`idcard`,`name`,`user_level_id`,`nickname`,`mobile`,`register_ip`,`weixin_openid`,`union_id`,`status` from cs_users where id = #{value}
	</select>

	<select id="list" resultType="com.neteye.health.domain.UsersDO">
		select a.`id`,a.`username`,a.`password`,a.`gender`,a.`marry`,a.`birthday`,a.`register_time`,a.`last_login_time`,a.`last_login_ip`,a.`type`,a.`company_id`,b.`company_name`,a.`idcard`,a.`name`,a.`user_level_id`,a.`nickname`,a.`mobile`,a.`register_ip`,a.`weixin_openid`,a.`union_id`,a.`status` from cs_users a,sys_company b
        <where>  
        			a.company_id = b.company_id
		  		  <if test="id != null and id != ''"> and a.id = #{id} </if>
		  		  <if test="username != null and username != ''"> and a.username = #{username} </if>
		  		  <if test="password != null and password != ''"> and a.password = #{password} </if>
		  		  <if test="gender != null and gender != ''"> and a.gender = #{gender} </if>
		  		  <if test="marry != null "> and a.marry = #{marry} </if>
		  		  <if test="birthday != null and birthday != ''"> and a.birthday = #{birthday} </if>
		  		  <if test="registerTime != null and registerTime != ''"> and a.register_time = #{registerTime} </if>
		  		  <if test="lastLoginTime != null and lastLoginTime != ''"> and a.last_login_time = #{lastLoginTime} </if>
		  		  <if test="lastLoginIp != null and lastLoginIp != ''"> and a.last_login_ip = #{lastLoginIp} </if>
		  		  <if test="type != null and type != ''"> and a.type = #{type} </if>
		  		  <if test="companyId != null and companyId != ''"> and a.company_id = #{companyId} </if>
		  		  <if test="idcard != null and idcard != ''"> and a.idcard = #{idcard} </if>
		  		  <if test="name != null and name != ''"> and a.name = #{name} </if>
		  		  <if test="userLevelId != null and userLevelId != ''"> and a.user_level_id = #{userLevelId} </if>
		  		  <if test="nickname != null and nickname != ''"> and a.nickname = #{nickname} </if>
		  		  <if test="mobile != null and mobile != ''"> and a.mobile = #{mobile} </if>
		  		  <if test="registerIp != null and registerIp != ''"> and a.register_ip = #{registerIp} </if>
		  		  <if test="weixinOpenid != null and weixinOpenid != ''"> and a.weixin_openid = #{weixinOpenid} </if>
		  		  <if test="unionId != null and unionId != ''"> and a.union_id = #{unionId} </if>
		  		  <if test="status != null "> and a.status = #{status} </if>
		  		</where>
        <choose>
            <when test="sort != null and sort.trim() != ''">
                order by ${sort} ${order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="count" resultType="int">
		select count(*) from cs_users
		 <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="username != null and username != ''"> and username = #{username} </if>
		  		  <if test="password != null and password != ''"> and password = #{password} </if>
		  		  <if test="gender != null and gender != ''"> and gender = #{gender} </if>
		  		  <if test="marry != null"> and marry = #{marry} </if>
		  		  <if test="birthday != null and birthday != ''"> and birthday = #{birthday} </if>
		  		  <if test="registerTime != null and registerTime != ''"> and register_time = #{registerTime} </if>
		  		  <if test="lastLoginTime != null and lastLoginTime != ''"> and last_login_time = #{lastLoginTime} </if>
		  		  <if test="lastLoginIp != null and lastLoginIp != ''"> and last_login_ip = #{lastLoginIp} </if>
		  		  <if test="type != null and type != ''"> and type = #{type} </if>
		  		  <if test="companyId != null and companyId != ''"> and company_id = #{companyId} </if>
		  		  <if test="idcard != null and idcard != ''"> and idcard = #{idcard} </if>
		  		  <if test="name != null and name != ''"> and name = #{name} </if>
		  		  <if test="userLevelId != null and userLevelId != ''"> and user_level_id = #{userLevelId} </if>
		  		  <if test="nickname != null and nickname != ''"> and nickname = #{nickname} </if>
		  		  <if test="mobile != null and mobile != ''"> and mobile = #{mobile} </if>
		  		  <if test="registerIp != null and registerIp != ''"> and register_ip = #{registerIp} </if>
		  		  <if test="weixinOpenid != null and weixinOpenid != ''"> and weixin_openid = #{weixinOpenid} </if>
		  		  <if test="unionId != null and unionId != ''"> and union_id = #{unionId} </if>
		  		  <if test="status != null "> and  status = #{status} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.neteye.health.domain.UsersDO" useGeneratedKeys="true" keyProperty="id">
		insert into cs_users
		(
			`username`, 
			`password`, 
			`gender`, 
			`marry`, 
			`birthday`, 
			`register_time`, 
			`last_login_time`, 
			`last_login_ip`, 
			`type`, 
			`company_id`, 
			`idcard`, 
			`name`, 
			`user_level_id`, 
			`nickname`, 
			`mobile`, 
			`register_ip`, 
			`weixin_openid`, 
			`union_id`,
			`status`
		)
		values
		(
			#{username}, 
			#{password}, 
			#{gender}, 
			#{marry}, 
			#{birthday}, 
			#{registerTime}, 
			#{lastLoginTime}, 
			#{lastLoginIp}, 
			#{type}, 
			#{companyId}, 
			#{idcard}, 
			#{name}, 
			#{userLevelId}, 
			#{nickname}, 
			#{mobile}, 
			#{registerIp}, 
			#{weixinOpenid}, 
			#{unionId},
			#{status}
		)
	</insert>
	 
	<update id="update" parameterType="com.neteye.health.domain.UsersDO">
		update cs_users 
		<set>
			<if test="username != null">`username` = #{username}, </if>
			<if test="password != null">`password` = #{password}, </if>
			<if test="gender != null">`gender` = #{gender}, </if>
			<if test="marry != null">`marry` = #{marry}, </if>
			<if test="birthday != null">`birthday` = #{birthday}, </if>
			<if test="registerTime != null">`register_time` = #{registerTime}, </if>
			<if test="lastLoginTime != null">`last_login_time` = #{lastLoginTime}, </if>
			<if test="lastLoginIp != null">`last_login_ip` = #{lastLoginIp}, </if>
			<if test="type != null">`type` = #{type}, </if>
			<if test="companyId != null">`company_id` = #{companyId}, </if>
			<if test="idcard != null">`idcard` = #{idcard}, </if>
			<if test="name != null">`name` = #{name}, </if>
			<if test="userLevelId != null">`user_level_id` = #{userLevelId}, </if>
			<if test="nickname != null">`nickname` = #{nickname}, </if>
			<if test="mobile != null">`mobile` = #{mobile}, </if>
			<if test="registerIp != null">`register_ip` = #{registerIp}, </if>
			<if test="weixinOpenid != null">`weixin_openid` = #{weixinOpenid}, </if>
			<if test="unionId != null">`union_id` = #{unionId},</if>
			<if test="status != null">`status` = #{status}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="remove">
		delete from cs_users where id = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from cs_users where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>